<small>Historique des commandes</small>
<table style="border: 1px solid black;width: 100%;margin-right: 10px">
	<thead style="background: rgb(146, 248, 146);">
		<tr>
			<td>N°</td>
			<td>Itinéraires</td>
		</tr>
	</thead>
	<tbody>
		{% for commande in commandes %}

			<tr style="vertical-align: top;background: white;padding-bottom: 0;">
				<td>{{ commande.id }}</td>
				<td>
					{% for itineraire in commande.client.itineraires %}
						<table style="border: 1px solid rgb(192, 192, 192);width: 100%;">
							<thead style="background: rgb(146, 248, 146);">
								<tr>
									<td>Designation</td>
									<td>Tarif</td>
									<td>Description</td>
								</tr>
							</thead>
							<tbody>
								<tr>
									<td>{{ itineraire.designation }}</td>
									<td>{{ itineraire.tarif ~ currency }}
									<td>{{ itineraire.description|raw }}</td>
								</tr>
							</tbody>
						</table>
					{% endfor %}
				</td>
			</tr>
		{% else %}
			<tr>
				<td colspan="4">Aucune commande</td>
			</tr>
		{% endfor %}

	</tbody>
</table>
