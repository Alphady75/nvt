                                        {% endfor %}
                                    </div>
                                    <div class="mb-3 border rounded p-3">
                                        <h5>Mail / sms</h5>
                                        {% for mailsms in client.mailSms %}
                                        <span class="text-uppercase">{{ mailsms }}</span>
                                        {% endfor %}
                                    </div>
                                    <div class="mb-3 border rounded p-3">
                                        <h5>Duplication / sms</h5>
                                        {% for duplication in client.duplicationSms %}
                                        <span class="text-uppercase">{{ duplication }}</span>
                                        {% endfor %}
