                                                    <td class="text-muted ">
                                                        {{ client.comptClientActVente }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Fact Aout</th>
                                                    <td class="text-muted ">
                                                        {{ client.comptClientFactAout  ? '<i class="fa fa-check text-success"></i>' : '' }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Certif</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-1">
                                                                <label for="" class="">Du</label>
                                                            </div>
                                                            <div class="col-sm-5">
                                                                {{ client.comptClientCertifStartAt|date('d/m/Y') }}
                                                            </div>
                                                            <div class="col-sm-1">
                                                                <label for="" class="">Au</label>
                                                            </div>
                                                            <div class="col-sm-5">
                                                                {{ client.comptClientCertifEndAt|date('d/m/Y') }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Reference</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                {{ client.comptClientRef }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                                <div class="col-sm-6">
                                    <div class="my-3 border p-3 rounded">
                                        <h5>Stat Quantité Cha</h5>
                                        <table class="table commande">
                                            <tbody>
                                                <tr>
                                                    <th>Tol attente cha</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-8">
                                                                {{ client.tolAttenteCha }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Tol retard cha</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-8">
                                                                {{ client.tolRetardCha }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Lors du cal attante (ch)</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-8">
                                                                {{ client.calAttanteCha }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>

                                            </tbody>
                                        </table>
                                    </div>

