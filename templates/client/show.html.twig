                                                        <div class="row">
                                                            <div class="col-sm-8">
                                                                {{ client.calAttanteLiv }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>

                                            </tbody>
                                        </table>
                                    </div>

                                    <div class="mb-3 border rounded p-3">
                                        <h5>Demandes clientes</h5>
                                        {% for demande in client.demandes %}
