{% extends 'base.html.twig' %}

{% block title %}Client -
{{ client.nom ~ ' ' ~ client.prenom }}
{% endblock %}

{% block links %}

<li class="breadcrumb-item">
    <a href="{{ path('client_index') }}">
        <i class="mdi mdi-account-group"></i>
        Clients</a>
</li>
<li class="breadcrumb-item active" aria-current="page">
    <i class="icon-user"></i>
    {{ client.nom ~ ' ' ~ client.prenom }}
</li>

{% endblock %}

{% block body %}

<div class="card bg-white">
    <div class="card-header bg-primary py-2">
        <h4 class="font-size-18 text-light">Informations du client:
            {{ client.nom ~ ' ' ~ client.prenom }}
        </h4>
    </div>
    <div class="card-body py-5">

        <div class="row">
            <div class="col-sm-11 mx-auto">

                <div class="client-form">
                    <div class="row">
                        <div class="col-sm-12 mx-auto">
                            <div class="row">
                                <div class="col-sm-6">

                                    <div class="my-3 border p-3 rounded">
                                        <table class="table commande">
                                            <h5>Général</h5>
                                            <tbody>
                                                <tr>
                                                    <th>CODE</th>
                                                    <td class="text-muted ">
                                                        {{ client.code }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>NUM</th>
                                                    <td class="text-muted ">
                                                        {{ client.num }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>siret</th>
                                                    <td class="text-muted ">
                                                        {{ client.siret }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>ape Naf</th>
                                                    <td class="text-muted ">
                                                        {{ client.apeNaf }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>num Tva</th>
                                                    <td class="text-muted ">
                                                        {{ client.numTva }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>type</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                {{ client.type }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>statut</th>
                                                    <td class="text-muted ">
                                                        {{ client.statut }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>raison Sociale</th>
                                                    <td class="text-muted ">
                                                        {{ client.raisonSociale }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>groupe Client</th>
                                                    <td class="text-muted ">
                                                        {{ client.groupeClient }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>sous Groupe</th>
                                                    <td class="text-muted ">
                                                        {{ client.sousGroupe }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>resp Facturation</th>
                                                    <td class="text-muted ">
                                                        {{ client.respFacturation }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>resp Saisie</th>
                                                    <td class="text-muted ">
                                                        {{ client.respSaisie }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>decodage Adr</th>
                                                    <td class="text-muted ">
                                                        {{ client.decodageAdr ? '<i class="fa fa-check text-success"></i>' : '' }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>adresse Valide</th>
                                                    <td class="text-muted ">
                                                        {{ client.adresseValide ? '<i class="fa fa-check text-success"></i>' : '' }}
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                    <div class="my-3 border p-3 rounded">
                                        <table class="table commande">
                                            <h5>Secteur</h5>
                                            <tbody>
                                                <tr>
                                                    <th>Appartenance</th>
