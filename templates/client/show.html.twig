                                            <tbody>
                                                <tr>
                                                    <th>Tr.CPT.</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                {{ client.comptClientTrCpt }}
                                                            </div>
