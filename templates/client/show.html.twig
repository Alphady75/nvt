                                                            <div class="col-sm-3">
                                                                {{ client.relanceJours|date('d/m/Y') }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Tel</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-5">
                                                                {{ client.relanceTel }}
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <label for="">Tel 2</label>
                                                            </div>
                                                            <div class="col-sm-5">
                                                                {{ client.relanceTel2 }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Email</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-8">
                                                                {{ client.relanceEmail }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Memo</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            {{ client.relanceMemo }}
                                                        </div>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                    <div class="my-3 border p-3 rounded">
