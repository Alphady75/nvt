                                                <tr>
                                                    <th>Val journalière</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                {{ client.facValJournaliere ? '<i class="fa fa-check text-success"></i>' : '' }}
                                                            </div>
                                                            <div class="col-sm-6">
                                                                {{ client.factTarif }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>edition</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                {{ client.edition }}
                                                            </div>
                                                            <div class="col-sm-6">
                                                                <div class="row">
                                                                    <div class="col-sm-6">
                                                                        {{ client.editionStartAt|date('H:i') }}
                                                                    </div>
                                                                    <div class="col-sm-6">
                                                                        {{ client.editionEndAt|date('H:i') }}
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td colspan="2">
                                                        {% for factMode in client.factMode %}
                                                        <span class="text-uppercase">{{ factMode }}</span>,
                                                        {% endfor %}
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                    <div class="">
                                        <h5>Momo transport</h5>
                                        {{ client.memoTrans }}
                                    </div>
                                    <br>
                                    <div class="">
                                        <h5>Indication feuille de route</h5>
                                        {{ client.indFeuilleRoute }}
                                    </div>

                                    <div class="my-3 border p-3 rounded">
                                        <table class="table commande">
                                            <h5>Adresse</h5>
                                            <tbody>
                                                <tr>
                                                    <th>Type adr</th>
                                                    <td class="text-muted ">
                                                        <div class="row">
                                                            <div class="col-sm-8">
                                                                {{ client.adresseType }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Code</th>
                                                    <td class="text-muted ">
                                                        {{ client.adresseCode }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Nom</th>
                                                    <td class="text-muted ">
                                                        {{ client.adresseNom }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Adresse</th>
                                                    <td class="text-muted ">
                                                        {{ client.adresse }}
