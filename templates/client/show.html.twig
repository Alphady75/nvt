                                        <div class="row">
                                            <div class="col-sm-6">
                                                <table class="table contact">
                                                    <tbody>
                                                        <tr>
                                                            <th>Chargement</th>
                                                            <td class="text-muted ">
                                                                {{ client.feuilleChar ? '<i class="fa fa-check text-success"></i>' : '' }}
