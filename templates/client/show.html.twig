                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                {{ client.sectAppartenance }}
                                                            </div>
                                                            <div class="col-sm-6">
                                                                {{ client.ville }}
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Compte</th>
                                                    <td class="text-muted ">
                                                        {{ client.sectCompte }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Secteur</th>
                                                    <td class="text-muted ">
                                                        {{ client.secteur }}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th>Num siret</th>
                                                    <td class="text-muted ">
                                                        {{ client.sectNumSiret }}
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                    <div class="my-3 border p-3 rounded">
